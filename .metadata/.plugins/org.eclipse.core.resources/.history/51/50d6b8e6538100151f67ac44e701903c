import static org.junit.Assert.*;

import java.util.TreeSet;

import org.junit.Test;

public class ECHashStringSetTest {

	/**Test empty constructor. */
	@Test
	public void testECHashStringSet() {
		ECHashStringSet test = new ECHashStringSet();
	}
	
	/**Test put method. */
	@Test
	public void testPut() {
		ECHashStringSet test = new ECHashStringSet();
		test.put("1Bumfuzzle");
		test.put("2Cattywampus");
		test.put("3Gardyloo");
		test.put("4Taradiddle");
		test.put("5Billingsgate");
		test.put("6Snickersnee");
		test.put("7Widdershins");
		test.put("8Collywobbles");
		test.put("9Gubbins");
		test.put("10Diphthong");
		test.put("10Diphthong");
		test.put("10Diphthong");
		test.put("11Diphthong");
		test.put("11Diphthong");
		test.put("11Diphthong");
		test.put("11Diphthong");
		test.put("11Diphthong");
		test.put("11Diphthong");
		test.put("11Diphthong");
		assertEquals(11, test.getNoOfEntry());
	}
	
	/**Test contains method. */
	@Test
	public void testContains() {
		ECHashStringSet test = new ECHashStringSet();
		test.put("Bumfuzzle");
		test.put("Cattywampus");
		test.put("Gardyloo");
		test.put("Taradiddle");
		test.put("Billingsgate");
		test.put("Snickersnee");
		test.put("Widdershins");
		test.put("Collywobbles");
		assertEquals(test.contains("Cattywampus"), true);
		assertEquals(test.contains("Nirvana"), false);
	}
	
	/**Test resize method. */
	@Test
	public void testResize() {
		ECHashStringSet test = new ECHashStringSet();
		assertEquals(5, test.getNoOfBucket());
		test.put("1");
		test.put("2");
		test.put("3");
		test.put("4");
		test.put("5");
		test.put("6");
		test.put("7");
		test.put("8");
		test.put("9");
		test.put("10");
		test.put("11");
		test.put("12");
		test.put("13");
		test.put("14");
		test.put("15");
		test.put("16");
		test.put("17");
		test.put("18");
		test.put("19");
		test.put("20");
		test.put("21");
		test.put("22");
		test.put("23");
		test.put("24");
		test.put("25");
		assertEquals(5, test.getNoOfBucket());
		test.put("26");
		assertEquals(25, test.getNoOfBucket());
	}
	
	/**Test huge input method. */
	@Test
	public void testHugeInput() {
		ECHashStringSet test = new ECHashStringSet();
		TreeSet<String> test2 = new TreeSet<String>();
		
		for(int i = 0 ; i <= 1000 ; i++) {
			String s = Long.toHexString(Double.doubleToLongBits(Math.random()));
			test.put(s);
			test2.add(s);
		}
		for(String s : test2) {
			assertEquals(true, test.contains(s));
		}
	}
	
	public static void main(String[] args) {
		org.junit.runner.JUnitCore.main("ECHashStringSetTest");
	}

}
